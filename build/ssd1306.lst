 204:Drivers/OLED/ssd1306.c ****         return;
 205:Drivers/OLED/ssd1306.c ****     }
 206:Drivers/OLED/ssd1306.c ****    
 207:Drivers/OLED/ssd1306.c ****     // Draw in the right color
 208:Drivers/OLED/ssd1306.c ****     if(color == White) {
 209:Drivers/OLED/ssd1306.c ****         SSD1306_Buffer[x + (y / 8) * SSD1306_WIDTH] |= 1 << (y % 8);
