 179:Drivers/OLED/ssd1306.c **** /* Write the screenbuffer with changed to the screen */
 180:Drivers/OLED/ssd1306.c **** void ssd1306_UpdateScreen(void) {
 181:Drivers/OLED/ssd1306.c ****     // Write data to each page of RAM. Number of pages
 182:Drivers/OLED/ssd1306.c ****     // depends on the screen height:
 183:Drivers/OLED/ssd1306.c ****     //
 184:Drivers/OLED/ssd1306.c ****     //  * 32px   ==  4 pages
 185:Drivers/OLED/ssd1306.c ****     //  * 64px   ==  8 pages
 186:Drivers/OLED/ssd1306.c ****     //  * 128px  ==  16 pages
 187:Drivers/OLED/ssd1306.c ****     for(uint8_t i = 0; i < SSD1306_HEIGHT/8; i++) {
 188:Drivers/OLED/ssd1306.c ****         ssd1306_WriteCommand(0xB0 + i); // Set the current RAM page address.
 189:Drivers/OLED/ssd1306.c ****         ssd1306_WriteCommand(0x00 + SSD1306_X_OFFSET_LOWER);
 190:Drivers/OLED/ssd1306.c ****         ssd1306_WriteCommand(0x10 + SSD1306_X_OFFSET_UPPER);
 191:Drivers/OLED/ssd1306.c ****         ssd1306_WriteData(&SSD1306_Buffer[SSD1306_WIDTH*i],SSD1306_WIDTH);
 192:Drivers/OLED/ssd1306.c ****     }
 193:Drivers/OLED/ssd1306.c **** }
