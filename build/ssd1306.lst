 215:Drivers/OLED/ssd1306.c **** /*
 216:Drivers/OLED/ssd1306.c ****  * Draw 1 char to the screen buffer
 217:Drivers/OLED/ssd1306.c ****  * ch       => char om weg te schrijven
 218:Drivers/OLED/ssd1306.c ****  * Font     => Font waarmee we gaan schrijven
 219:Drivers/OLED/ssd1306.c ****  * color    => Black or White
 220:Drivers/OLED/ssd1306.c ****  */
 221:Drivers/OLED/ssd1306.c **** char ssd1306_WriteChar(char ch, SSD1306_Font_t Font, SSD1306_COLOR color) {
 222:Drivers/OLED/ssd1306.c ****     uint32_t i, b, j;
 223:Drivers/OLED/ssd1306.c ****     
 224:Drivers/OLED/ssd1306.c ****     // Check if character is valid
 225:Drivers/OLED/ssd1306.c ****     if (ch < 32 || ch > 126)
 226:Drivers/OLED/ssd1306.c ****         return 0;
 227:Drivers/OLED/ssd1306.c ****     
 228:Drivers/OLED/ssd1306.c ****     // Char width is not equal to font width for proportional font
 229:Drivers/OLED/ssd1306.c ****     const uint8_t char_width = Font.char_width ? Font.char_width[ch-32] : Font.width;
 230:Drivers/OLED/ssd1306.c ****     // Check remaining space on current line
 231:Drivers/OLED/ssd1306.c ****     if (SSD1306_WIDTH < (SSD1306.CurrentX + char_width) ||
 232:Drivers/OLED/ssd1306.c ****         SSD1306_HEIGHT < (SSD1306.CurrentY + Font.height))
 233:Drivers/OLED/ssd1306.c ****     {
 234:Drivers/OLED/ssd1306.c ****         // Not enough space on current line
 235:Drivers/OLED/ssd1306.c ****         return 0;
 236:Drivers/OLED/ssd1306.c ****     }
 237:Drivers/OLED/ssd1306.c ****     
 238:Drivers/OLED/ssd1306.c ****     // Use the font to write
 239:Drivers/OLED/ssd1306.c ****     for(i = 0; i < Font.height; i++) {
 240:Drivers/OLED/ssd1306.c ****         b = Font.data[(ch - 32) * Font.height + i];
 241:Drivers/OLED/ssd1306.c ****         for(j = 0; j < char_width; j++) {
 242:Drivers/OLED/ssd1306.c ****             if((b << j) & 0x8000)  {
 243:Drivers/OLED/ssd1306.c ****                 ssd1306_DrawPixel(SSD1306.CurrentX + j, (SSD1306.CurrentY + i), (SSD1306_COLOR) col
 244:Drivers/OLED/ssd1306.c ****             } else {
 245:Drivers/OLED/ssd1306.c ****                 ssd1306_DrawPixel(SSD1306.CurrentX + j, (SSD1306.CurrentY + i), (SSD1306_COLOR)!col
 246:Drivers/OLED/ssd1306.c ****             }
 247:Drivers/OLED/ssd1306.c ****         }
 248:Drivers/OLED/ssd1306.c ****     }
 249:Drivers/OLED/ssd1306.c ****     
 250:Drivers/OLED/ssd1306.c ****     // The current space is now taken
 251:Drivers/OLED/ssd1306.c ****     SSD1306.CurrentX += char_width;
 252:Drivers/OLED/ssd1306.c ****     
 253:Drivers/OLED/ssd1306.c ****     // Return written char for validation
 254:Drivers/OLED/ssd1306.c ****     return ch;
 255:Drivers/OLED/ssd1306.c **** }
